<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edu.cqupt.shemining.mapper.RegionMapper">
    <select id="checkProvince" resultType="java.lang.String">
        select province from region;
    </select>

    <select id="checkCity" resultType="java.lang.String">
        select city from region;
    </select>

    <select id="checkCounty" resultType="java.lang.String">
        select county from region;
    </select>
</mapper>